# Solar AI Agent - Environment Variables
# Copy this file and add your actual API keys

# AI & Calling
OPENROUTER_API_KEY=your_openrouter_key_here
TWILIO_ACCOUNT_SID=your_twilio_sid_here
TWILIO_AUTH_TOKEN=your_twilio_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_here

# Email Services
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email_here
SMTP_PASS=your_email_password_here

# CRM Integration
HUBSPOT_API_KEY=your_hubspot_key_here

# Social Media APIs
LINKEDIN_CLIENT_ID=your_linkedin_client_id_here
LINKEDIN_CLIENT_SECRET=your_linkedin_secret_here
META_ACCESS_TOKEN=your_meta_token_here

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/solar_ai_agent

# App Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:8000

# Webhook Security
WEBHOOK_SECRET=your_webhook_secret_here
